<!--Start Body************************************************* -->

    <!--title="實體卡片線上儲值" border="normal"-->
    <z:window id="OnlinePinCodeDeposit_Window" use="com.apt.epay.ui.OnlinePinCodeDeposit_Window" border="none">
        <custom-attributes scope="request" org.zkoss.zk.desktop.nocache="true"/>
        <z:separator/>
        <z:auxhead>
            <z:auxheader label=" " style="height:20px;"/>
        </z:auxhead>
        <z:div align="center" id="div_sms_conf">
           
            <z:groupbox width="80%" contentStyle="border:0">
                <z:vbox>
                    <z:separator spacing="5px"/>
					<table class="table-default">

                    <tr>
					<td width="250px" height="50px" class="aligh-left" style="background: #EEEEEE; line-height: 30px; border-bottom:1px solid #e0e0e0;">
                        <div style="font-family: '微軟正黑體';font-size: 20px; ">
						<z:image src="/img/icon_red.gif"/>
                        <z:label id="label_contract_mdn"/> 
						</div>
					</td>
					<td colspan="3">
                        <z:textbox id="textbox_contract_mdn" width="200px" disabled = "true" class="word_infor" style="font-family: '微軟正黑體';font-size: 20px;"/>
					</td>
					</tr>
					<tr>

					<td width="250px" height="50px" class="aligh-left" style="background: #EEEEEE; line-height: 30px; border-bottom:1px solid #e0e0e0;">
                        <div style="font-family: '微軟正黑體';font-size: 20px; ">
						<z:image src="/img/icon_red.gif"/>
                        <z:label id="label_mdn"/> 

						</div>
					</td>
					<td colspan="3">
                        <z:textbox id="textbox_mdn" width="200px" maxlength="10" constraint="no empty,/^[0-9]{10}/: 門號需完整10碼數字" disabled = "false" class="word_infor" style="font-family: '微軟正黑體';font-size: 20px;"/>
                        <z:label value="　"/>
                        <z:button id="chkBtn" label="品牌查詢" onClick="OnlinePinCodeDeposit_Window.chkPromotionCode()"/>

                        <div style="font-family: '微軟正黑體';font-size: 10px; ">
                        請再次確認您將儲值之4G預付卡門號，以免因輸入錯誤，無法使用。
                        </div>
					</td>
					</tr>
					<tr>
					<td width="250px" height="50px" class="aligh-left" style="background: #EEEEEE; line-height: 30px; border-bottom:1px solid #e0e0e0;">
                        <div style="font-family: '微軟正黑體';font-size: 20px; ">
						<z:image src="/img/icon_red.gif"/>
                        儲值卡密碼(Password)
						</div>
					</td>
					<td colspan="3">
                        <z:textbox id="textbox_pincode" width="200px" disabled = "false" style="font-family: '微軟正黑體';font-size: 20px;" class="word_infor" constraint="no empty,/^[0-9]{14,16}/: 需完整14或16碼數字"/>
					</td>
					</tr>
					<tr>
					<td width="250px" height="50px" class="aligh-left" style="background: #EEEEEE; line-height: 30px; border-bottom:1px solid #e0e0e0;">
                        <div style="font-family: '微軟正黑體';font-size: 20px; ">
						<z:image src="/img/icon_red.gif"/>
                        檢核碼(Valid Code)
						</div>
					</td>
					<td colspan="3">
                        <z:textbox id="chkcode"  width="200px" disabled = "false" class="word_infor" style="font-family: '微軟正黑體';font-size: 20px;"/>
                        <z:captcha id="cpa" length="5" width="150px" height="45px" exclude="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ" onClick="cpa.randomValue();chkcode.setFocus(true);" class="word_bottom" />
					</td>
					</tr>
					</table>
                </z:vbox>

            </z:groupbox>
        </z:div>
            
        <z:div align="center">
            <z:button label="確認" id="sendPinCodeBtn" autodisable="self" style="border-radius: 10px; padding: 5px 15px; border: none; font-size: 20px; color: #fff; background: rgb(255, 204, 102); /* Old browsers */
		background: -moz-linear-gradient(top, rgba(255, 204, 102, 1) 0%, rgba(255, 153, 0, 1) 100%); /* FF3.6+ */
		background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, rgba(255, 204, 102, 1)), color-stop(100%, rgba(255, 153, 0, 1))); /* Chrome, Safari4+ */
		background: -webkit-linear-gradient(top, rgba(255, 204, 102, 1) 0%, rgba(255, 153, 0, 1) 100%); /* Chrome10+, Safari5.1+ */
		background: -o-linear-gradient(top, rgba(255, 204, 102, 1) 0%, rgba(255, 153, 0, 1) 100%); /* Opera 11.10+ */
		background: -ms-linear-gradient(top, rgba(255, 204, 102, 1) 0%, rgba(255, 153, 0, 1) 100%); /* IE10+ */
		background: linear-gradient(to bottom, rgba(255, 204, 102, 1) 0%, rgba(255, 153, 0, 1) 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffcc66', endColorstr='#ff9900', GradientType=0 ); /* IE6-9 */">
                <attribute name="onClick">
                    textbox_mdn.getValue();
                    textbox_pincode.getValue();
                    OnlinePinCodeDeposit_Window.sendOrder();
                </attribute>
            </z:button>
            <z:button label="網站立即購買儲值" id="sendURLBtn" style="border-radius: 10px; padding: 5px 15px; border: none; font-size: 20px; color: #fff; background: rgb(255, 204, 102); /* Old browsers */
		background: -moz-linear-gradient(top, rgba(255, 204, 102, 1) 0%, rgba(255, 153, 0, 1) 100%); /* FF3.6+ */
		background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, rgba(255, 204, 102, 1)), color-stop(100%, rgba(255, 153, 0, 1))); /* Chrome, Safari4+ */
		background: -webkit-linear-gradient(top, rgba(255, 204, 102, 1) 0%, rgba(255, 153, 0, 1) 100%); /* Chrome10+, Safari5.1+ */
		background: -o-linear-gradient(top, rgba(255, 204, 102, 1) 0%, rgba(255, 153, 0, 1) 100%); /* Opera 11.10+ */
		background: -ms-linear-gradient(top, rgba(255, 204, 102, 1) 0%, rgba(255, 153, 0, 1) 100%); /* IE10+ */
		background: linear-gradient(to bottom, rgba(255, 204, 102, 1) 0%, rgba(255, 153, 0, 1) 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffcc66', endColorstr='#ff9900', GradientType=0 ); /* IE6-9 */">
                <attribute name="onClick">
                    OnlinePinCodeDeposit_Window.sendURL();
                </attribute>
            </z:button>
 
        </z:div>
     
    </z:window>
    
<!--End Body************************************************* -->
